<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC	"-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.module.live.model.Together" table="template_together">
		<id name="id" column="ID_">
			<generator class="increment"/>
		</id>
		<property name="name" column="NAME_"/>
		<property name="address" column="ADDRESS_"/>
		<property name="averagePrice" column="AVERAGE_PRICE"/>
		<property name="tenPrice" column="TEN_PRICE"/>
		<property name="sex" column="SEX"/>
		<property name="title" column="TITLE_"/>
		<property name="createTime" column="CREATE_TIME"/>
		<property name="createId" column="CREATE_ID"/>
		<property name="status" column="STATUS_"/>
	</class>

	<class name="com.module.live.model.House" table="template_house">
		<id name="id" column="ID_">
			<generator class="increment"/>
		</id>
		<property name="name" column="NAME_"/>
		<property name="price" column="PRICE_"/>
		<property name="type" column="TYPE_"/>
		<property name="area" column="AREA_"/>
		<property name="position" column="POSITION_"/>
		<property name="title" column="TITLE_"/>
		<property name="createTime" column="CREATE_TIME"/>
		<property name="createId" column="CREATE_ID"/>
		<property name="status" column="STATUS_"/>
	</class>

</hibernate-mapping>