<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC	"-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.module.template.model.BaseTemplate" table="template_model">
		<id name="id" column="ID_">
			<generator class="increment"/>
		</id>
		<property name="name" column="NAME_"/>
		<property name="templateType" column="TEMPLATE_TYPE"/>
		<property name="templateName" column="TEMPLATE_NAME"/>
		<property name="moduleName" column="MODULE_NAME"/>
		<property name="savePathModel" column="SAVE_PATH_MODEL"/>
		<property name="saveFileModel" column="SAVE_FILE_MODEL"/>
		<property name="title" column="TITLE_"/>
		<property name="createTime" column="CREATE_TIME"/>
		<property name="createId" column="CREATE_ID"/>
		<property name="status" column="STATUS_"/>
	</class>

	<class name="com.module.template.model.Module" table="template_module">
		<id name="id" column="ID_">
			<generator class="increment"/>
		</id>
		<property name="name" column="NAME_"/>
		<property name="className" column="CLASS_NAME"/>
		<property name="title" column="TITLE_"/>
		<property name="createTime" column="CREATE_TIME"/>
		<property name="createId" column="CREATE_ID"/>
		<property name="status" column="STATUS_"/>
	</class>

	<class name="com.module.template.model.Attribute" table="template_attribute">
		<id name="id" column="ID_">
			<generator class="increment"/>
		</id>
		<property name="name" column="NAME_"/>
		<property name="code" column="CODE_"/>
		<property name="type" column="TYPE_"/>
		<property name="title" column="TITLE_"/>
		<property name="moduleId" column="MODULE_ID"/>
		<property name="createTime" column="CREATE_TIME"/>
		<property name="createId" column="CREATE_ID"/>
		<property name="status" column="STATUS_"/>
	</class>

	<class name="com.module.template.model.Action" table="template_action">
		<id name="id" column="ID_">
			<generator class="increment"/>
		</id>
		<property name="name" column="NAME_"/>
		<property name="code" column="CODE_"/>
		<property name="title" column="TITLE_"/>
		<property name="moduleId" column="MODULE_ID"/>
		<property name="returnType" column="RETURN_TYPE"/>
		<property name="paramTypes" column="PARAM_TYPES"/>
		<property name="paramNames" column="PARAM_NAMES"/>
		<property name="actionSql" column="ACTION_SQL"/>
		<property name="createTime" column="CREATE_TIME"/>
		<property name="createId" column="CREATE_ID"/>
		<property name="status" column="STATUS_"/>
	</class>

</hibernate-mapping>